<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://editorCaption"
        class="com.company.sample.web.airports_5.AirportEdit"
        datasource="airportDs"
        focusComponent="fieldGroup"
        messagesPack="com.company.sample.web.airports_5">
    <dsContext>
        <datasource id="airportDs"
                    class="com.company.sample.entity.airports.Airport"
                    view="_minimal">
            <collectionDatasource id="dutyFreeShopsDs"
                                  property="dutyFreeShops"/>
        </datasource>
    </dsContext>
    <dialogMode height="600"
                width="800"/>
    <layout expand="windowActions"
            spacing="true">
        <fieldGroup id="fieldGroup"
                    datasource="airportDs">
            <column width="250px">
                <field property="name"/>
            </column>
        </fieldGroup>
        <frame id="windowActions"
               screen="editWindowActions"/>
    </layout>
</window>
